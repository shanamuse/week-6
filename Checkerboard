#include <stdio.h>

int main() {
    int rows;
    printf("Rows?\n");
    scanf("%d", &rows);

    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < 8; j++) {
            if ((i + j) % 2 == 0) {
                printf("black ");
            } else {
                printf("white ");
            }
        }
        printf("\n");
    }

    return 0;
}
